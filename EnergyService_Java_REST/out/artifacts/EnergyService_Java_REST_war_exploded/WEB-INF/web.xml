<?xml version="1.0" encoding="UTF-8"?>
<web-app xmlns="http://java.sun.com/xml/ns/javaee"
         xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
         xsi:schemaLocation="http://java.sun.com/xml/ns/javaee http://java.sun.com/xml/ns/javaee/web-app_2_5.xsd"
         version="2.5">
    <servlet-mapping>
        <servlet-name>DataStoreServlet_getData</servlet-name>
        <url-pattern>/getData</url-pattern>
    </servlet-mapping>

    <servlet>
        <servlet-name>DataStoreServlet_getData</servlet-name>
        <servlet-class>DataStoreServlet_getData</servlet-class>
    </servlet>

    <servlet-mapping>
        <servlet-name>DataStoreServlet_create</servlet-name>
        <url-pattern>/create</url-pattern>
    </servlet-mapping>

    <servlet>
        <servlet-name>DataStoreServlet_create</servlet-name>
        <servlet-class>DataStoreServlet_create</servlet-class>
    </servlet>
</web-app>